{
	"name": "Condenser",
	"version": "{{version}}",
	"manifest_version": 3,
	"description": "Used only by me to help me with my website distractions",
	"background": {
		"service_worker": "service-worker.js",
		"type": "module"
	},
	"permissions": ["webNavigation", "storage", "declarativeNetRequest", "declarativeNetRequestFeedback"],
	"host_permissions": ["<all_urls>"],
	"action": {
		"default_popup": "initializationError.html"
	},
	"externally_connectable": {
		"matches": ["<all_urls>"]
	}
}
